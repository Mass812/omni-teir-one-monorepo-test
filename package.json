{
  "name": "ts-omni",
  "version": "0.1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:Mass812/omni-teir-one-monorepo-test.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "NPM_REGISTRY_AND_PACKAGES": "echo 'NPM_REGISTRY_AND_PACKAGES SECTION',",
    "local:reregister": "npm run remove:nm && npm run remove:dist && npm run install:all && npm run build:all",
    "remove:local_registry": "rimraf ~/.local/share/verdaccio",
    "publish": "npx lerna exec 'npm publish --registry http://localhost:4873'",
    "local-config": "verdaccio --config ./verdaccio/config.yaml && npm config set registry http://localhost:4873",
    "npm-config": "npm config set registry https://registry.npmjs.org",
    "publish:npm": "echo 'TODO: publish with Lerna to private NPM'",
    "DEVELOPMENT_SECTION": "echo 'DEVELOPMENT_SECTION SECTION',",
    "scaffold": "node scripts/generate/generate.js $1 $2",
    "link": "npx lerna link",
    "graph": "npx nx graph",
    "lerna:clean": "npx lerna clean --yes && npx nx reset",
    "BUILD_INSTALL_AND_DELETE": "echo 'BUILD, INSTALL && DELETE SECTION',",
    "install:all": "npm i && npm run link && npx lerna exec npm i",
    "build": "npx lerna exec tsc && npm run pretty",
    "remove:dist": "npx lerna run remove:dist",
    "remove:nm": "npx lerna run remove:nm",
    "clean-projects": "npm run remove:nm && npm run remove:dist",
    "regenerate": "npm run nuke && npm run create",
    "TEST_CHECK_AND_FORMAT": "echo 'TEST_CHECK_AND_FORMAT SECTION',",
    "build:check": "tsc --noEmit",
    "test": "npx lerna exec jest",
    "lint": "npx eslint . --ext .ts --fix",
    "pretty": "npx prettier --write .",
    "format": "npm run lint && npm run pretty",
    "git:commit": "git add . && git cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "8.0.0",
    "nx": "15.3.3"
  },
  "peerDependencies": {
    "reflect-metadata": "0.1.13",
    "rxjs": "7.1.0",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@nestjs/schematics": "8.0.0",
    "@nestjs/testing": "8.0.0",
    "@nrwl/nx-cloud": "15.0.2",
    "@types/jest": "27.5.2",
    "@types/node": "18.11.15",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.29.0",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "27.5.1",
    "lerna": "6.1.0",
    "nx": "15.3.3",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "scripty": "2.1.1",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run test && npm run build:check"
    }
  }
}
